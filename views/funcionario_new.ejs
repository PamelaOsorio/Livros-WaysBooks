  <body>
    <header id="cabecalho" alt="cabecalho" style="width: auto;">
        <p class="font-weight-bold" style="font-family:'Berkshire Swash'; color: rgb(0, 0, 0); font-size: 48px; border-color: tomato; border-left: 2px; "><img style="width: 80px;" src="/./images/icons8-livros-100.png">WaysBooks</p>

               
    </header>
    <div class="alert alert-danger" role="alert">
        Preencha o campo de cadastro abaixo 
    </div>
    <body style="background: rgb(184, 137, 196);">
        <div class="container">
    
            <form action="/funcionarios" method="POST" enctype="multipart/form-data">
                <div class="form-group">
     <form>
        
        <div class="form-row">
            <div class="col-md-2 offset-md-0">
                <label for="name" class="text-black">Nome</label>
                <input type="text" class="form-control" placeholder="nome" name="name"  required>

     
            </div>
            <div class="col-md-2 offset-md-0">
                <label for="datanasc" class="text-black">Data de Nascimento</label>
                <input type="date" class="form-control"  name="datanasc"  required>

            </div>
        </div>
        <div class="form-row">
            <div class="col-md-4 offset-md-0">
                <label for="email" class="text-black">E-mail</label>
                <input type="email" class="form-control" placeholder="email" name="email" required>
            </div>
        <div class="form-row">
            <div class="col-md-4 offset-md-0">
                <label for="senha" class="text-black">Senha</label>
                <input type="password" class="form-control" placeholder="senha" name="senha" required>
            </div>
            <div class="col-md-5 offset-md-0">
                <label for="C_senha" class="text-black">Confirmar senha</label>
                <input type="password" class="form-control" placeholder="confirmar a senha" name="C_senha" required>
            </div>
        </div>
        </div>
        <br>
        <div class="col-md-5 offset-md-0">
        <div class="form-row">
            <button type="submit" class="btn btn-outline-danger">Salvar</button>

        </div>
        </div>
    </div>
               

          
          
